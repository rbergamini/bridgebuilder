# Uncomment this line to define a global platform for your project
platform :ios, '8.0'

target 'BridgeBuilder' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'FirebaseUI', '~> 4.0'

  # Pods for BridgeBuilder

  target 'BridgeBuilderTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BridgeBuilderUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
